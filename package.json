{
  "name": "scraps-js",
  "version": "0.0.2",
  "description": "Interactive Code Editing in Browser",
  "main": "scraps.js",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "package": "tsc && mkdir -p dist && cat lib/jsonview.js lib/prism.js scraps.js > dist/prepack.js && webpack && cat lib/jsonview.css lib/prism.css lib/scraps.css > dist/scraps.css",
    "postpack": "google-closure-compiler --js=dist/postpack.js --js_output_file=dist/scraps.js && rm dist/postpack.js && rm dist/prepack.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arkamedus/scraps-js.git"
  },
  "author": "Gordon Goodrum",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/arkamedus/scraps-js/issues"
  },
  "homepage": "https://github.com/arkamedus/scraps-js#readme",
  "dependencies": {
    "acorn": "^8.0.4",
    "webpack": "^5.10.0"
  },
  "devDependencies": {
    "@types/node": "^14.14.10",
    "closure-webpack-plugin": "^2.3.0",
    "webpack-cli": "^4.2.0"
  }
}
